{
  "short_name": "Project scrapy",
  "name": "scrapy the happy scraper",
  "version": "1.0.0",
  "description": "â€¦",

  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_title": "popup",
    "css": "style.css"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [],
  "permissions": ["declarativeContent", "activeTab", "tabs"]
}
